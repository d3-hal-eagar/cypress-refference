{
  "name": "credmo-end2end-cypress-cucumber",
  "version": "1.0.0",
  "description": "end to end testing with cypress and cucumber",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/cypress run --spec cypress/integration/ci/**/*",
    "test": "node_modules/.bin/cypress open",
    "test:mobile": "node_modules/.bin/cypress open --config viewportWidth=360,viewportHeight=640 --env TESTMODE=mobile",
    "cypress:open": "node_modules/.bin/cypress open",
    "cypress:headless": "node_modules/.bin/cypress run",
    "cypress:record": "node_modules/.bin/cypress run --record --key f3230691-58fd-4a68-9a0d-9ceb252be8e6",
    "cypress:mobilerecord": "node_modules/.bin/cypress run  --config viewportWidth=360,viewportHeight=640 --env TESTMODE=mobile --record --key f3230691-58fd-4a68-9a0d-9ceb252be8e6",
    "runGroup:ci-end2end": "node_modules/.bin/cypress run --spec cypress/integration/ci/end2end/**/*",
    "runGroup:ci-component": "node_modules/.bin/cypress run --spec cypress/integration/ci/component/**/*"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.systemadmin.com/door3/credmo-end2end.git"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "testing"
  ],
  "author": "Hal Eagar",
  "license": "",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "cypress-cucumber-preprocessor": "^1.2.1",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.2",
    "mochawesome-report-generator": "^3.1.2"
  },
  "devDependencies": {
    "cypress": "^3.0.3"
  }
}
