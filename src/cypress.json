{
  "projectId": "cwb5ky",
  "pageLoadTimeout": 100000,
  "watchForFileChanges": false,
  "trashAssetsBeforeRuns": true,
  "video": false,
  "viewportHeight":	768,
  "viewportWidth":	1024,
  "baseUrl": "http://localhost:4200",
  "reporter": "junit",
  "reporterOptions": {
    "mochaFile": "cypress/reports/[hash]-cypress-junit-output.xml",
    "toConsole": false,
    "properties": {
      "Enviroment": "local-docker",
      "Platform": "Win 10"
    },
    "suiteTitleSeparatedBy": " Scenario: ",
    "includePending": true,
    "rootSuiteTitle": "credmo",
    "testsuitesTitle": "Cypress Tests",
    "testCaseSwitchClassnameAndName": true,
    "useFullSuiteTitle": false
  },
  "integrationFolder": "cypress/integration/",
  "waitForAnimations": true,
  "env": {
    "TESTMODE": "desktop"
  }
}
